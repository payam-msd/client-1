{
  "name": "DastanIran",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.css",
    "start": "npm-run-all --parallel watch:sass",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.final.css --output-style compressed",
    "gh-pages": "gh-pages -d /",
    "build": "npm-run-all compile:sass prefix:css compress:css gh-pages "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/payam-msd/Natours-pure-CSS.git"
  },
  "author": "payamdev",
  "license": "ISC",
  "bugs": {
    "url": "git@github.com:payam-msd/DASTANIRAN-client.git"
  },
  "homepage": "https://github.com/payam-msd/DASTANIRAN-client#readme",
  "dependencies": {
    "autoprefixer": "^9.4.9",
    "concat": "^1.0.3",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  }
}
